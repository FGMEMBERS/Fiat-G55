<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <fx>
    
    <crank>
      <name>engstart1</name>
      <path>Aircraft/Fiat-G55/Sounds/starter.wav</path>
      <property>/engines/engine[0]/cranking</property>
      <volume>
        <offset>0.4</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <crank>
      <name>crank1</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <property>/engines/engine[0]/cranking</property>
        <not>
          <property>/engines/engine[0]/running</property>
        </not>
      </condition>
      <volume>
        <offset>0.5</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <crank>
      <name>cough1</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <offset>0.3</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <engine>
      <name>engine1</name>
      <mode>looped</mode>
      <path>Aircraft/Fiat-G55/Sounds/engine.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <property>/engines/engine[0]/mp-osi</property>
        <factor>0.005</factor>
        <min>0.15</min>
        <max>0.5</max>
        <offset>0.15</offset>
      </volume>
      <pitch>
        <property>/engines/engine[0]/rpm</property>
        <factor>0.0012</factor>
        <min>1.0</min>
        <max>5.0</max>
        <offset>0.3</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
    
    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Aircraft/Fiat-G55/Sounds/runway.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.5</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>
    
    <gear>
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Aircraft/Fiat-G55/Sounds/gears.wav</path>
      <property>/gear/gear[0]/position-norm</property>
      <pitch>
        <internal>dt_play</internal>
        <offset>1.0</offset>
        <factor>-0.001</factor>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>
    
    <gear>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <greather-than>
          <property>/gear/gear[0]/position-norm</property>
          <value>0.01</value>
        </greather-than>
        <less-than>
          <property>/gear/gear[0]/position-norm</property>
          <value>0.1</value>
        </less-than>
      </condition>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>
    
    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
        <min>0.1</min>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>0.4</offset>
      </volume>
      <pitch>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>
    
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/Fiat-G55/Sounds/flaps.wav</path>
      <property>/surface-positions/flaps-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </flap>
    
    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <min>0.03</min>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>
    
    <stall>
      <name>stall</name>
      <path>Sounds/stall.wav</path>
      <property>/sim/aero/alarms/stall-warning</property>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <min>30.0</min>
      </volume>
    </stall>
    
    <guns> 
      <name>gunfire</name>
      <mode>looped</mode>
      <path>Aircraft/Fiat-G55/Sounds/gunsfire.wav</path>
      <condition> 
        <property>controls/armament/trigger</property>
        <property>ai/submodels/submodel/count</property>
      </condition>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </guns>
    
    <canopy> 
      <name>canopy slide</name>
      <mode>in-transit</mode>
      <path>Aircraft/Fiat-G55/Sounds/canopy_slide.wav</path>
      <property>sim/model/door-positions/crew/position-norm</property>
      <volume> 
        <factor>0.3</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </canopy>
    
    <!-- AIRFRAME TORSION from IAR 80-->
    <high-g>
      <name>high-g</name>
      <mode>in-transit</mode>
      <path>Aircraft/Fiat-G55/Sounds/airframe-torsion.wav</path>
      <condition>
        <or>
          <greater-than>
            <property>accelerations/pilot-g</property>
            <value>2.2</value>
          </greater-than>
          <less-than>
            <property>accelerations/pilot-g</property>
            <value>-0.9</value>
          </less-than>
        </or>
        <property>/sim/current-view/internal</property>
      </condition>
      <volume>
        <property>accelerations/pilot-g</property>
        <factor>0.08</factor>
      </volume>
      <pitch>
        <property>accelerations/pilot-g</property>
        <factor>0.02</factor>
        <max>0.2</max>
      </pitch>
      <position>
        <x-m> 2.573</x-m>
        <y-m> 0.000</y-m>
        <z-m>-0.200</z-m>
      </position>
      <reference-dist>2.5</reference-dist>
      <max-dist>20</max-dist>
    </high-g>
    
  </fx>
  
</PropertyList>
